        <com.hahalolo.incognito.presentation.conversation.view.sticker.favorite.IncognitoMsgFavoriteView
            android:id="@+id/favorite"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />


        <com.hahalolo.incognito.presentation.conversation.view.sticker.emoji.IncognitoMsgEmojiView
            android:id="@+id/emoji"
