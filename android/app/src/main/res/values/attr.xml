<resources>
    <declare-styleable name="BookingHotelV2DetailItem">
        <attr name="show_count_guest" format="boolean" />
        <attr name="show_delete" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BookingRoomV2GuestDetailItem">
        <attr name="title" format="string" />
        <attr name="enable_edit" format="boolean"/>
